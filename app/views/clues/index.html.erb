<a href="logout">Logout</a>
<br>
<p>Your team is currently on clue <%= @clue_num %>.</p>
<div class="log_in">
    <%= form_tag(:action => :question_handler, :method => "get") do %> 
        <p><%= label_tag(:text_val, "Please enter in your answer for this clue:") %></p> 
        <p>Answer: <%= text_field_tag(:answer) %></p>
        <p><%= submit_tag("Submit") %></p>
    <% end %>
</div>
